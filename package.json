{
  "scripts": {
    "sv:prd": "NODE_ENV=production pnpm run start",
    "sv:dev": "NODE_ENV=development pnpm run start",
    "build:prd": "pnpm build -- --jsx react-jsx",
    "build:dev": "pnpm build",
    "build": "tsc",
    "start": "node server.js",
    "publish-gh-pages": "git checkout gh-pages && git merge -q master && pnpm build:prd && git add -f build/ && git commit -m 'gh-pages' && git push --set-upstream origin gh-pages && git checkout master"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "typescript": "^4.6.2"
  },
  "type": "module"
}
